{
  "name": "2fa-worker",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "build:web": "cd web && npm install && npm run build",
    "build:worker": "wrangler deploy --dry-run --outdir web/dist && node -e \"require('fs').renameSync('web/dist/index.js', 'web/dist/_worker.js')\"",
    "build": "npm run build:web && npm run build:worker",
    "deploy": "npm run build && wrangler pages deploy web/dist"
  },
  "dependencies": {
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.0.0",
    "typescript": "^5.0.0",
    "wrangler": "^3.114.17"
  }
}